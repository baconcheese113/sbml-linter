{
  "name": "sbml-linter",
  "displayName": "sbml-linter",
  "description": "Lints SBML files using libSBML",
  "version": "1.0.3",
  "publisher": "joseph-yannessa",
  "icon": "sbml-linter-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/baconcheese113/sbml-linter.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:xml",
    "onLanguage:sbml",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "fast-xml-parser": "^5.2.1"
  },
  "files": [
    "out/**/*",
    "validator/windows/sbml_validator.exe",
    "validator/linux/sbml_validator",
    "validator/linux/lib/*",
    "validator/mac/sbml_validator",
    "validator/mac/lib/*",
    "LICENSE.md",
    "README.md",
    "sbml-linter-icon.png"
  ],
  "volta": {
    "node": "22.15.0"
  }
}
